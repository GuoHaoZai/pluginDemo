<idea-plugin>
    <id>guohao.code.generator</id>
    <name>myGenerator</name>
    <vendor email="1347194676@qq.com" url="http://www.weli.cn" />

    <description>常用的模版代码生成器</description>

    <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.lang</depends>
    <depends>com.intellij.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
        <intentionAction>
            <className>guohao.code.generator.actions.impl.GenerateSetterAction</className>
        </intentionAction>
        <intentionAction>
            <className>guohao.code.generator.actions.GenerateBuilderAction</className>
        </intentionAction>

        <intentionAction>
            <className>guohao.code.generator.actions.impl.GenerateSetterNoDefaultValueAction</className>
        </intentionAction>

        <intentionAction>
            <className>guohao.code.generator.actions.impl.GenerateSetterFromArgsAction</className>
        </intentionAction>
    </extensions>
</idea-plugin>